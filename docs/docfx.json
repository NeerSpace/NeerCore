{
  "metadata": [
    {
      "src": [{"src": "../src/NeerCore", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore"
    },
    {
      "src": [{"src": "../src/NeerCore.Api", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Api"
    },
    {
      "src": [{"src": "../src/NeerCore.Application.MediatR", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Application.MediatR"
    },
    {
      "src": [{"src": "../src/NeerCore.Data", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Data"
    },
    {
      "src": [{"src": "../src/NeerCore.Data.Abstractions", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Data.Abstractions"
    },
    {
      "src": [{"src": "../src/NeerCore.Data.EntityFramework", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Data.EntityFramework"
    },
    {
      "src": [{"src": "../src/NeerCore.DependencyInjection", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.DependencyInjection"
    },
    {
      "src": [{"src": "../src/NeerCore.Logging.NLog", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Logging.NLog"
    },
    {
      "src": [{"src": "../src/NeerCore.Mapping.Mapster", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Mapping.Mapster"
    },
    {
      "src": [{"src": "../src/NeerCore.Typeids", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Typeids"
    },
    {
      "src": [{"src": "../src/NeerCore.Typeids.Api", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Typeids.Api"
    },
    {
      "src": [{"src": "../src/NeerCore.Typeids.Api.Hashids", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Typeids.Api.Hashids"
    },
    {
      "src": [{"src": "../src/NeerCore.Typeids.Data.EntityFramework", "files": ["**.csproj"]}],
      "dest": "obj/api/NeerCore.Typeids.Data.EntityFramework"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**.yml"],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [
          "**.md",
          "**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["assets/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["api/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/darkfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_enableSearch": true,
      "_appLogoPath": "assets/favicon.png",
      "_appFaviconPath": "assets/favicon.png",
      "_appTitle": "NeerCore",
      "_appFooter": "Copyright (c) @jurilents"
    }
  }
}